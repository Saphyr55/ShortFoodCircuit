<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<fx:root type="AnchorPane" xmlns:fx="http://javafx.com/fxml" fx:controller="fr.sfc.controller.ConnectionController">
    <children>
        <TextField fx:id="identifierTextField" cache="true" cacheHint="QUALITY" layoutX="96.0" layoutY="89.0" prefHeight="51.0" prefWidth="209.0" promptText="Identifier">
            <font>
                <Font name="Arial" size="24.0" />
            </font>
        </TextField>
        <PasswordField fx:id="passwordTextField" layoutX="96.0" layoutY="199.0" prefHeight="46.0" prefWidth="209.0" promptText="Password">
            <font>
                <Font name="Arial" size="24.0" />
            </font>
        </PasswordField>
        <Button onAction="#onPressLogin" alignment="CENTER" layoutX="128.0" layoutY="322.0" mnemonicParsing="false" prefHeight="45.0" prefWidth="144.0" text="Login">
            <font>
                <Font name="Arial" size="19.0" />
            </font>
        </Button>
        <Label alignment="CENTER" layoutX="99.0" layoutY="418.0" prefHeight="38.0" prefWidth="200.0">
            <font>
                <Font name="Arial" fx:id="errorMessage" size="26.0" />
            </font>
        </Label>
    </children>
    <opaqueInsets>
        <Insets />
    </opaqueInsets>
</fx:root>
