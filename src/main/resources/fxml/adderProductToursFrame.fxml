                <Label layoutX="20.0" layoutY="82.0" text="End date"/>
                <Label layoutX="20.0" layoutY="116.0" text="weight"/>
                <Label layoutX="20.0" layoutY="150.0" text="SIRET Company"/>
                <Label layoutX="20.0" layoutY="182.0" text="Matriculation"/>
