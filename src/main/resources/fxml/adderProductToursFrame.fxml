<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.layout.HBox?>

<fx:root type="HBox" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fr.sfc.controller.productTour.ProductToursFrameController">
    <children>
        <VBox fx:id="containerLabel">
            <Label text="name"/>
            <Label text="Start date"/>
            <Label text="End date"/>
            <Label text="weight"/>
            <Label text="SIRET Company"/>
            <Label text="Matriculation"/>
        </VBox>
        <VBox fx:id="containerTextField">
            <TextField fx:id="tFName" />
            <TextField fx:id="tFWeight" />
            <TextField fx:id="tfSIRETCompany" />
            <TextField fx:id="tFMatriculation" />
            <DatePicker fx:id="startDate"/>
            <DatePicker fx:id="endDate"/>
        </VBox>
        <VBox>
            <Button fx:id="buttonFinish" onAction="#EventButtonAddProductToursFinishAction" text="Finish"/>
        </VBox>
    </children>
</fx:root>
